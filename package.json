  "devDependencies": {
    "@types/jest": "23.3.1",
    "@types/node": "^8.5.9",
    "@types/winston": "^2.3.7",
    "jest": "23.5.0",
    "prettier": "^1.11.1",
    "request": "^2.83.0",
    "ts-jest": "23.1.3",
    "tslint": "5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.6.2"
