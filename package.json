    "etag": "^1.7.0",
    "fakeredis": "^2.0.0",
    "i18n": "^0.8.3",
    "ioredis": "^4.0.0",
    "is-running": "^2.1.0",
    "log": "^3.2.0",
    "mime": "^2.0.1",
    "mocha": "^3.4.1",
    "node-resque": "^4.0.7",
    "primus": "^7.3.2",
    "qs": "^6.5.0",
    "sails-memory": "^0.10.7",
    "should": "^13.0.0",
    "source-map-support": "^0.5.0",
