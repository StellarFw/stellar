    "etag": "1.8.1",
    "fakeredis": "2.0.0",
    "handlebars": "4.0.11",
    "i18n": "^0.8.3",
    "ioredis": "^3.2.2",
    "is-running": "^2.1.0",
    "lodash": "^4.17.4",
    "log": "^1.4.0",
    "mime": "^2.3.1",
    "node-resque": "5.2.0",
    "primus": "7.2.0",
    "qs": "6.5.2",
    "redis-commands": "1.3.5",
    "sails-disk": "1.0.0-12",
    "source-map-support": "0.5.4",
    "st-formidable": "^1.0.18",
    "sywac": "^1.2.0",
    "then-fakeredis": "^2.0.1",
    "uglify-es": "3.3.9",
    "uuid": "3.2.1",
