{
  "name": "stellar-fw",
  "author": "Gil Mendes <gil00mendes@gmail.com>",
  "version": "1.1.1",
  "description": "A modular Web framework based on actions, to create Web APIs easily",
  "keywords": [
    "api",
    "web framework",
    "stellar",
    "rest",
    "http",
    "https",
    "tcp",
    "action",
    "task",
    "websocket"
  ],
  "scripts": {
    "dev": "rm -rf ./dist && babel --watch --source-maps --out-dir ./dist/ ./src/",
    "build": "rm -rf ./dist && babel --out-dir ./dist/ ./src/",
    "test": "npm run lint && node test.js",
    "test:travis": "NODE_ENV=test ./node_modules/istanbul/lib/cli.js cover ./node_modules/.bin/_mocha",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "eslint src"
  },
  "dependencies": {
    "async": "^2.1.4",
    "babel-polyfill": "^6.16.0",
    "bcrypt": "^1.0.2",
    "browser_fingerprint": "0.1.0",
    "etag": "^1.7.0",
    "fakeredis": "^2.0.0",
    "st-formidable": "^1.0.18",
    "handlebars": "^4.0.6",
    "i18n": "^0.8.3",
    "ioredis": "^2.4.2",
    "is-running": "^2.1.0",
    "log": "^1.4.0",
    "mime": "^1.3.4",
    "node-resque": "^4.0.5",
    "primus": "^6.0.6",
    "qs": "^6.0.1",
    "sails-memory": "^0.10.7",
    "source-map-support": "^0.4.0",
    "uglify-js": "^2.6.4",
    "uuid": "^3.0.0",
    "waterline": "^0.11.6",
    "winston": "^2.1.1",
    "ws": "^2.0.3",
    "yargs": "^7.0.1",
    "yargonaut": "^1.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.14.0",
    "coveralls": "^2.11.9",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "istanbul": "^0.4.3",
    "mocha": "^3.2.0",
    "request": "^2.65.0",
    "should": "^11.1.1"
  },
  "repository": {
    "type": "git",
